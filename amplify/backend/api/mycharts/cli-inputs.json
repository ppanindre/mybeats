{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "mycharts",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "API_KEY",
      "keyDescription": "api key description",
      "expirationTime": 30
    },
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "OPTIMISTIC_CONCURRENCY"
      }
    },
    "additionalAuthTypes": [
      {
        "mode": "AWS_IAM"
      }
    ]
  }
}